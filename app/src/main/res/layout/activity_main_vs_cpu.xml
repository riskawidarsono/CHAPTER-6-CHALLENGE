<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivityVsCpu">

    <TextView
        android:id="@+id/tv_gamesuit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Games Suit"
        android:textSize="50sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_pemenang"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center_horizontal"
        android:text="PEMENANG"
        android:textColor="#008577"
        android:textSize="50sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/guideline_Hori"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_gamesuit"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_pemain1"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Pemain 1"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/guideline_Hori"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/tv_pemain2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="Pemain 2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/guideline_Hori" />

    <ImageView
        android:id="@+id/iv_batu"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/batu"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_pemain1"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_batu1"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/batu"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/tv_pemain2" />

    <ImageView
        android:id="@+id/iv_gunting"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/gunting"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_batu"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_vs"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="210dp"
        android:src="@drawable/vs"
        app:layout_constraintEnd_toStartOf="@+id/guideline1"
        app:layout_constraintStart_toStartOf="@+id/guideline"
        app:layout_constraintTop_toTopOf="@+id/guideline_Hori" />

    <ImageView
        android:id="@+id/iv_gunting1"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/gunting"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/iv_batu1" />

    <ImageView
        android:id="@+id/iv_kertas"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/kertas"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_gunting"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/iv_exit"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_exit"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/guideline"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/iv_kertas" />

    <ImageView
        android:id="@+id/iv_kertas1"
        android:layout_width="0dp"
        android:layout_height="96dp"
        android:layout_marginTop="50dp"
        android:src="@drawable/kertas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/iv_gunting1" />

    <ImageView
        android:id="@+id/iv_restart"
        android:layout_width="0dp"
        android:layout_height="50dp"
        android:src="@drawable/ic_cached"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/guideline1"
        app:layout_constraintTop_toBottomOf="@+id/iv_kertas1"
        app:showAsAction="always" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline_Hori"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="horizontal"
        app:layout_constraintGuide_percent="0.33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.33" />

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/guideline1"
        android:layout_width="0dp"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="271dp" />

</androidx.constraintlayout.widget.ConstraintLayout>